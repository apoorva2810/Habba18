<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">


    <RelativeLayout
        android:id="@+id/relative_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:background="#8b8b8b8b"
        android:layout_marginTop="190dp"
        android:orientation="vertical">

        <com.github.mmin18.widget.RealtimeBlurView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:realtimeBlurRadius="100dp"
            app:realtimeOverlayColor="#3cb0b0b0"/>


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="#3cb0b0b0"/>


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="1dp">


                    <TextView
                        android:text="Working"
                        android:layout_width="match_parent"
                        android:id="@+id/name"
                        android:layout_gravity="bottom"
                        android:layout_height="wrap_content"
                        android:textAlignment="center"
                        android:gravity="center"
                        android:textSize="35sp"
                        android:background="#00000000"
                        android:textAllCaps="false"
                        android:textColor="@android:color/white"
                        android:paddingBottom="6dp"
                        android:transitionName="text"/>

                </LinearLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="1dp"
                    android:background="#00000000"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp"
                        android:paddingTop="15dp"
                        android:text="Description"
                        android:textColor="@android:color/white"
                        android:textSize="20sp"/>

                    <TextView
                        android:id="@+id/about"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="20dp"
                        android:textColor="@android:color/white"
                        android:textSize="15sp"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/white"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="1dp"
                    android:background="#00000000"
                    android:orientation="vertical">

                    <com.github.mmin18.widget.RealtimeBlurView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:realtimeBlurRadius="100dp"
                        app:realtimeOverlayColor="@android:color/transparent"/>

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp"
                        android:paddingTop="15dp"
                        android:text="Rules and Regulation"
                        android:textColor="@android:color/white"
                        android:textSize="20sp"/>

                    <TextView
                        android:id="@+id/rules"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="20dp"
                        android:textColor="@android:color/white"
                        android:textSize="15sp"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/white"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="1dp"
                    android:background="#00000000"
                    android:orientation="vertical">

                    <com.github.mmin18.widget.RealtimeBlurView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:realtimeBlurRadius="100dp"
                        app:realtimeOverlayColor="@android:color/transparent"/>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="10dp"
                            android:paddingLeft="5dp"
                            android:paddingTop="15dp"
                            android:text="Contact Number"
                            android:textColor="@android:color/white"
                            android:textSize="20sp"/>

                        <ImageView
                            android:id="@+id/callerbut"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toEndOf="@id/textView7"
                            android:layout_toRightOf="@id/textView7"
                            android:src="@drawable/callerblue"/>

                        <ImageView
                            android:id="@+id/whatsapp_button"
                            android:layout_marginLeft="5dp"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:src="@drawable/whatsapp"
                            android:layout_alignParentBottom="true"
                            android:layout_toEndOf="@id/callerbut"
                            android:layout_toRightOf="@id/callerbut"/>
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/ename"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="20dp"
                        android:textColor="@android:color/white"
                        android:textSize="15sp"/>


                    <TextView
                        android:id="@+id/number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"


                        android:layout_gravity="bottom|left"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"

                        android:layout_marginRight="12dp"
                        android:textColor="@android:color/white"
                        />


                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/white"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="1dp"
                    android:background="#00000000"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:paddingLeft="5dp"
                            android:paddingTop="15dp"
                            android:text="Venue"
                            android:textColor="@android:color/white"
                            android:textSize="20sp"/>

                        <ImageView
                            android:id="@+id/button_navigate"
                            android:layout_width="25dp"
                            android:layout_marginLeft="5dp"
                            android:layout_height="25dp"
                            android:layout_alignParentBottom="true"
                            android:layout_toEndOf="@id/textView11"
                            android:layout_toRightOf="@id/textView11"
                            android:src="@drawable/map"/>

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/venue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="20dp"
                        android:textColor="@android:color/white"
                        android:textSize="15sp"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/white"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="1dp"
                    android:background="#00000000"
                    android:orientation="vertical">

                    <com.github.mmin18.widget.RealtimeBlurView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:realtimeBlurRadius="100dp"
                        app:realtimeOverlayColor="@android:color/transparent"/>

                    <TextView
                        android:id="@+id/textView8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp"
                        android:paddingTop="15dp"
                        android:text="Amount"
                        android:textColor="@android:color/white"
                        android:textSize="20sp"/>

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="10dp"
                        android:text="Registration Amount:"
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/amount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="10dp"
                        android:textColor="@android:color/white"
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="10dp"
                        android:text="Price Money:"
                        android:textColor="@android:color/white"
                        android:textSize="16sp"
                        />

                    <TextView
                        android:id="@+id/pmoney"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_gravity="bottom|center"
                        android:layout_marginBottom="6dp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="12dp"
                        android:paddingTop="10dp"
                        android:textColor="@android:color/white"
                        android:textSize="17sp"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="10dp"
                    android:background="@android:color/white"/>

                <Button
                    android:id="@+id/button_register"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    android:layout_gravity="center"
                    android:text="REGISTER"
                    android:textColor="#70bdba"
                    android:background="@drawable/shape_button"/>

            </LinearLayout>


        </ScrollView>


    </RelativeLayout>

</RelativeLayout>